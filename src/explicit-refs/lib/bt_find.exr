let n_left = { data <= 12; left <= 0; right <= 0}    (* 0 in head signals null *)
in let n_right = { data <= 44; left <= 0; right <= 0}
in let n_root = { data <= 33; left <= n_left ; right <= n_right }
in let t1 = { root <= n_root ; length <= 3} 
in letrec finding (goal) = proc (nd) {
    if number?(nd)
    then 0
    else (if nd.data = goal 
        then 1 
        else ((finding goal) nd.left) + ((finding goal) nd.right))
    }
    in let find_bt = proc (t) {proc (val) { (((finding val) t.root) >> 0)  } }
    in ((find_bt t1) 12)